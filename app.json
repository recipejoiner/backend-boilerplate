{
  "name": "Rails Devise GraphQL",
  "description": "This is a boilerplate to build your next Saas product. It's a RubyOnRails 6 API only backend with Authentication and GrpahQL API.",
  "keywords": [
    "saas",
    "saas-boilerplate",
    "ruby-on-rails",
    "rails6",
    "devise",
    "graphql",
    "jwt",
    "ruby-graphql",
    "rails-boilerplate",
    "rails-api-only",
    "api-only"
  ],
  "website": "https://gatsby-redux.zauberware.com/",
  "repository": "https://github.com/zauberware/rails-devise-graphql",
  "logo": "https://avatars3.githubusercontent.com/u/1753330?s=200&v=4",
  "success_url": "/",
  "scripts": {
    "postdeploy": "rake db:migrate && rake db:seed"
  },
  "env": {
    "DEVISE_JWT_SECRET_KEY": {
      "description": "A secret key for verifying the integrity of signed JWT tokens.",
      "generator": "run 'rails secret'"
    },
    "ADMIN_EMAIL": {
      "description": "Admin default login.",
      "value": "ari@mendelow.me"
    },
    "ADMIN_PASSWORD": {
      "description": "Admin default password.",
      "value": "Demo1234!!"
    },
    "ADMIN_FIRST_NAME": {
      "description": "Admin default first name.",
      "value": "Ari"
    },
    "ADMIN_LAST_NAME": {
      "description": "Admin default last name.",
      "value": "Mendelow"
    }
  },
  "image": "heroku/ruby",
  "addons": [
    {
      "plan": "heroku-postgresql:hobby-dev"
    }
  ]
}